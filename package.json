{
    "name": "todotoday",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "concurrently -n fe,be \"cd frontend && yarn run start\" \"wait-on http://localhost:3003 && cd backend && yarn run start\"",
        "open": "open backend/dist-electron/mac-arm64",
        "build": "cd frontend && yarn run build && cd ../backend && yarn run build && cd .. && yarn run open",
        "cleanup": "rm -rf node_modules && rm -rf ./backend/node_modules && rm -rf ./frontend/node_modules",
        "bootstrap": "yarn && cd backend && yarn && cd ../frontend && yarn"
    },
